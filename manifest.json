{
  "name": "New Tab in Group",
  "description": "Open a new tab in the same group as the current tab.",
  "version": "1.0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "new-tab-in-group": {
      "description": "Open New Tab in Group",
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      }
    }
  },
  "icons": {
    "128": "icon.png"
  }
}
